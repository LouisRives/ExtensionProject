{
  "manifest_version": 3,
  "name": "Rule Extractor Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
		"default_popup": "popup.html",
		 "default_icon": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
